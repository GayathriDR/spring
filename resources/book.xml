<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
	<bean class="java.lang.Integer" id="integer">
	<constructor-arg value="5"/>
	</bean>
	
	<bean class="java.lang.Double" id="double">
	<constructor-arg value="900.00"/>
	</bean>
	
	<bean class="com.xworkz.propertydatabase.Book" id="book">
	<property name="bookName" value="complete refrence"/>
	<property name="bookId" ref="integer"/>
	<property name="bookPrice" ref="double"/>
	</bean>
	</beans>
	
	